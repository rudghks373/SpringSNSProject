package com.mvcboard.board.Dto;

public class ContentDto {


    private String id;
    private String name;
    private String pass;
	public ContentDto() {
		// TODO Auto-generated constructor stub
	}
	
	public ContentDto(String id, String name, String pass) {
		this.id=id;
		this.name = name;
		this.pass = pass;
	}



}